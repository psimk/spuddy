{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@demo/*": ["src/demo/*"],
      "@app/*": ["src/app/*"],
      "@shared/*": ["src/shared/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
