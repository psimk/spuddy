{
  "name": "spuddy",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "dev": "astro dev",
    "dev:clean": "rm -rf node_modules/{.astro, .vite} ; node --run dev",
    "format": "prettier --write .",
    "instant:login": "instant-cli login",
    "instant:push": "instant-cli push",
    "preview": "astro preview",
    "typecheck": "tsc --noEmit --incremental"
  },
  "dependencies": {
    "@astrojs/partytown": "^2.1.4",
    "@dnd-kit/dom": "^0.1.21",
    "@dnd-kit/helpers": "^0.1.21",
    "@dnd-kit/react": "^0.1.21",
    "@instantdb/react": "^0.22.2",
    "@lucide/astro": "^0.546.0",
    "fractional-indexing-jittered": "^1.0.0",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.4"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^12.6.10",
    "@astrojs/react": "^4.4.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.14",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vite-pwa/astro": "^1.1.1",
    "astro": "^5.14.6",
    "daisyui": "^5.3.7",
    "instant-cli": "^0.22.21",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.14"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-tailwindcss"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
